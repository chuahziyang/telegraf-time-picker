{
    "name": "telegraf-time-picker",
    "version": "1.0.7",
    "description": "Based on telegraf, this class generate a simple, inline-keyboard time picker.",
    "keywords": [
        "telegram",
        "bot",
        "telegraf"
    ],
    "homepage": "https://github.com/noobj/telegraf-time-picker#readme",
    "bugs": {
        "url": "https://github.com/noobj/telegraf-time-picker/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/noobj/telegraf-time-picker.git"
    },
    "license": "Apache-2.0",
    "author": "noobj",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.ts",
    "files": [
        "src/**/*.ts",
        "dist/**/*.js",
        "dist/**/*.d.ts",
        "dist/**/*.d.ts.map"
