        "@typescript-eslint/parser": "^5.11.0",
        "dotenv": "^16.0.0",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.5.1",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.5.0",
        "tsc-watch": "^4.6.0",
        "tsconfig-paths": "^3.12.0",
        "typescript": "^4.5.5"
    }
